<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<tokenFilters>
    <tokenizer>whitespace</tokenizer>
    <tokenFilter>
        <name>lowercase</name>
    </tokenFilter>
    <tokenFilter>
        <name>stop</name>
    </tokenFilter>
    <tokenFilter>
        <name>worddelimiter</name>
        <param>
            <key>preserveOriginal</key>
            <value>1</value>
        </param>
        <param>
            <key>generateWordParts</key>
            <value>1</value>
        </param>
    </tokenFilter>
    <tokenFilter>
        <name>shingle</name>
        <param>
            <key>minShingleSize</key>
            <value>2</value>
        </param>
        <param>
            <key>maxShingleSize</key>
            <value>3</value>
        </param>
    </tokenFilter>

</tokenFilters>
